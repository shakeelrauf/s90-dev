<div class="modal fade" id="myPlaylists" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" >
  <div class="modal-dialog" role="document">
    <div class="modal-content" style="background-color: #2c3f50;">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"><%= t("client.common.p11")%></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <ul class="list-of-playlists" style="overflow-y: scroll">

        </ul>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal"><%= t("client.common.p10")%></button>
      </div>
    </div>
  </div>
</div>