<div class="event__like event_liked_unliked" style="z-index: 5;" data-liked="<%= event["liked"] %>" data-event-id="<%= event["event_id"] %>" id="event_liked_unliked<%= event["event_id"] %>">
  <% if event["liked"] == true %>
      <img src="<%= image_url 'client/svg/heart-2.svg'%>" id="like-img-<%= event["event_id"] %>" width="32" alt="">
  <% else %>
    <img src="<%= image_url 'client/svg/heart.svg'%>" id="like-img-<%= event["event_id"] %>"  width="32" alt="">
  <% end %>
</div><!-- /.event__like -->